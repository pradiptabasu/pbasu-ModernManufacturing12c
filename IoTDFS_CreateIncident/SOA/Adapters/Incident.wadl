<?xml version='1.0' encoding='UTF-8'?>
<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://xmlns.oracle.com/IoT_DFS/IoTDFS_CreateIncident/Incident" xmlns:ns1="http://xmlns.oracle.com/IoT_DFS/IoTDFS_CreateIncident/CreateIncident" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="Incident">RestService to Create Incident in RightNow</doc>
   <grammars>
      <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
         <xsd:import namespace="http://xmlns.oracle.com/IoT_DFS/IoTDFS_CreateIncident/CreateIncident" schemaLocation="../Schemas/CreateIncident.xsd"/>
      </xsd:schema>
   </grammars>
   <resources>
      <resource path="/">
         <method name="POST" soa:wsdlOperation="CreateIncident">
            <request>
               <representation mediaType="application/xml" element="cns:CreateIncidentReq" xmlns:cns="http://xmlns.oracle.com/IoT_DFS/IoTDFS_CreateIncident/CreateIncident"/>
               <representation mediaType="application/json" element="cns:CreateIncidentReq" xmlns:cns="http://xmlns.oracle.com/IoT_DFS/IoTDFS_CreateIncident/CreateIncident"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:IncidentResponse" xmlns:cns="http://xmlns.oracle.com/IoT_DFS/IoTDFS_CreateIncident/CreateIncident"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
